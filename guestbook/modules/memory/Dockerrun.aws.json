{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "guestbook",
            "image": "quintilesims/guestbook",
            "essential": true,
            "memory": 128,
            "environment": [
                {
                    "name": "BACKEND_TYPE",
                    "value": "${backend_type}"
                },
                {
                    "name": "BACKEND_CONFIG",
                    "value": "${backend_config}"
                }
            ],
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ]
        }
    ]
}
